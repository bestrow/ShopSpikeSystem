<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.libo.msproject.dao.MsproductinfoDao">

    <insert id="insertMsproductinfo" parameterType="msproductinfo">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO msproductinfo(productid,producttitle,productpicture,originalprice,miaoshaprice,
        merchantid,applydate,auditdate,auditstate,starttime,endtime,productcount,stockcount,descrioption)
        VALUES (#{productid},#{producttitle},#{productpicture},#{originalprice},#{miaoshaprice},#{merchantid},#{applydate},
        #{auditdate},#{auditstate},#{starttime},#{endtime},#{productcount},#{stockcount},#{descrioption})
    </insert>


</mapper>